    "bunyan": "^1.8.12",
    "bunyan-debug-stream": "^1.0.8",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "compression": "^1.7.0",
    "connect-mongo": "^2.0.1",
    "css-loader": "^0.28.7",
    "csurf": "^1.9.0",
    "debug": "^3.0.1",
