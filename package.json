  "author": "Aleksandr Ignatyev <vigntom.2708@gmail.com>",
  "engines": {
    "node": "9.4.0",
    "npm": "5.4.2",
    "yarn": "1.3.2"
  },
  "scripts": {
    "start": "heroku local",
    "server": "nodemon --watch 'app' --watch 'config' --watch 'public' --watch 'lib' --ext 'js,ejs,json' --exec 'heroku local' --signal SIGTERM --delay 0.3",
    "test": "ava",
    "test:watch": "ava --watch",
    "build": "webpack --env.production --progress",
    "build:vendor": "webpack --env.production --env.vendor --progress",
    "build:full": "yarn build:vendor && yarn build",
    "build:dev": "webpack --env.development --progress",
    "build:dev:watch": "webpack --env.development --progress --watch",
    "build:dev:vendor": "webpack --env.development --env.vendor",
    "build:dev:full": "yarn build:dev:vendor && yarn build:dev",
    "heroku-postbuild": "yarn build:full"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.22.0",
    "dotenv": "^4.0.0",
    "jsdom": "^11.5.1",
    "nodemon": "^1.14.8",
    "reload": "^2.2.2",
    "standard": "^10.0.3",
    "supertest": "^3.0.0"
  },
  "ava": {
    "tap": false,
    "color": true,
    "verbose": true,
    "require": [
      "./test/helpers/setup-env.js"
    ]
  },
  "dependencies": {
    "autoprefixer": "^7.1.6",
