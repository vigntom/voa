{
  "name": "voa",
  "version": "0.0.1",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigntom2708/voa.git"
  },
  "author": "Aleksandr Ignatyev <vigntom.2708@gmail.com>",
  "engines": {
    "node": "8.9.1",
    "npm": "5.4.2",
    "yarn": "1.2.1"
  },
  "scripts": {
    "start": "heroku local",
    "server": "nodemon --watch 'app' --watch 'config' --watch 'public' --ext 'js,ejs,json' --exec 'heroku local' --signal SIGTERM",
    "test": "NODE_ENV=test ava",
    "test:watch": "NODE_ENV=test ava --watch",
    "build": "webpack --env.production --progress",
    "build:vendor": "webpack --env.production --env.vendor --progress",
    "build:full": "yarn build:vendor && yarn build",
    "build:dev": "webpack --env.development --progress",
    "build:dev:vendor": "webpack --env.development --env.vendor",
    "build:dev:full": "yarn build:dev:vendor && yarn build:dev"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "ava": "^0.22.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.0.0-beta.2",
    "browser-env": "^3.2.1",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "jquery": "^3.2.1",
    "node-sass": "^4.6.0",
    "nodemon": "^1.12.1",
    "popper.js": "^1.12.6",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "reload": "^2.2.2",
    "sass-loader": "^6.0.6",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "supertest": "^3.0.0",
    "tern": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^4.1.1"
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "require": [
      "./test/helpers/setup-browser-env.js"
    ],
    "tap": false,
    "color": true,
    "fail-fast": true,
    "verbose": true
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "bunyan-debug-stream": "^1.0.8",
    "compression": "^1.7.0",
    "csurf": "^1.9.0",
    "debug": "^3.0.1",
    "ejs": "^2.5.7",
    "ejs-loader": "^0.3.0",
    "express": "^4.15.4",
    "express-validator": "^4.2.0",
    "helmet": "^3.8.1",
    "hyperscript-helpers": "^3.0.3",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hyperscript": "^3.0.0",
    "throng": "^4.0.0"
  }
}
