  "author": "Aleksandr Ignatyev <vigntom.2708@gmail.com>",
  "engines": {
    "node": "9.10.1"
  },
  "scripts": {
    "start": "heroku local",
    "build": "webpack --env.production",
    "build:vendor": "webpack --env.production --env.vendor",
    "build:full": "yarn build:vendor && yarn build",
    "dev:start": "nodemon --watch './' --ext 'js,ejs,json' --exec 'heroku local' --signal SIGTERM --delay 0.5",
    "dev:build": "webpack --env.development --progress",
    "dev:build:watch": "webpack --env.development --progress --watch",
    "dev:build:vendor": "webpack --env.development --env.vendor",
    "dev:build:full": "yarn dev:build:vendor && yarn dev:build",
    "db:addAdmin": "node ./scripts/add-admin",
    "db:drop": "node ./scripts/drop-db",
    "db:seed": "node ./config/db/seeds.js",
    "test": "ava",
    "test:watch": "ava --watch",
    "secret": "node ./scripts/secret",
    "heroku-postbuild": "yarn build:full && yarn db:addAdmin"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.25.0",
    "cookie": "^0.3.1",
    "dotenv": "^5.0.1",
    "faker": "^4.1.0",
    "jsdom": "^11.5.1",
    "mongodb-memory-server": "^1.6.5",
    "nodemon": "^1.14.8",
    "reload": "^2.2.2",
    "standard": "^11.0.1",
    "supertest": "^3.0.0"
  },
  "ava": {
    "tap": false,
    "color": true,
    "verbose": true,
    "timeout": "60s",
    "require": [
      "./test/helpers/setup-env.js"
    ]
  },
  "dependencies": {
    "autoprefixer": "^8.2.0",
